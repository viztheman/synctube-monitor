{
  "name": "synctube-monitor",
  "version": "1.0.2",
  "main": "main.js",
  "author": "Daniel Hammond <viztheman@protonmail.com>",
  "license": "MIT",
  "private": "true",
  "description": "Monitor a synctube instance for outages.",
  "repository": "https://github.com/viztheman/synctube-monitor.git",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@electron-forge/publisher-github": "^6.0.0-beta.67",
    "electron": "^21.2.0",
    "electronmon": "^2.0.2"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "watch": "electronmon ."
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cheerio": "^1.0.0-rc.12",
    "electron-json-storage": "^4.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "knockout": "^3.5.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "synctube_monitor"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "viztheman",
              "name": "synctube-monitor"
            }
          },
          "draft": true
        }
      ]
    }
  }
}
